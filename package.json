{
  "name": "modular-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build modular-app",
    "build:libs": "ng build --configuration production halved && ng build --configuration production doubled",
    "postbuild:libs": "npm run package:halved && npm run package:doubled",
    "build:count-write": "ng build count-write --configuration production --output-hashing=none",
    "postbuild:count-write": "cat dist/count-write/*.js | gzip > dist/count-write/count-write.js.gz",
    "build:count-read": "mkdir -p dist/count-read && cat projects/count-read/*.js | gzip > dist/count-read/count-read.js.gz",
    "serve:count-write": "http-server dist/count-write --gzip -p 8080",
    "serve:count-read": "http-server dist/count-read --gzip -p 80801",
    "package:halved": "cd dist/halved && npm pack",
    "package:doubled": "cd dist/doubled && npm pack",
    "install:libs": "npm run install:halved && npm run install:doubled",
    "install:halved": "npm i dist/halved/devdbe-halved-0.0.1.tgz",
    "install:doubled": "npm i dist/doubled/devdbe-doubled-0.0.1.tgz",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.8",
    "@angular/common": "~12.2.8",
    "@angular/compiler": "~12.2.8",
    "@angular/core": "~12.2.8",
    "@angular/forms": "~12.2.8",
    "@angular/platform-browser": "~12.2.8",
    "@angular/platform-browser-dynamic": "~12.2.8",
    "@angular/router": "~12.2.8",
    "@devdbe/doubled": "file:dist/doubled/devdbe-doubled-0.0.1.tgz",
    "@devdbe/halved": "file:dist/halved/devdbe-halved-0.0.1.tgz",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.8",
    "@angular/cli": "~12.2.7",
    "@angular/compiler-cli": "~12.2.8",
    "@types/node": "^12.11.1",
    "ng-packagr": "^12.1.0",
    "typescript": "~4.3.2"
  }
}